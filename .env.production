# Production Environment Configuration
# Generated: 2026-01-16

# GitHub OAuth Configuration
REACT_APP_GITHUB_CLIENT_ID=YOUR_GITHUB_CLIENT_ID
REACT_APP_GITHUB_REDIRECT_URI=https://trancendos.github.io/secrets-portal/callback

# API Configuration
REACT_APP_API_BASE_URL=https://api.github.com
REACT_APP_API_TIMEOUT=30000

# Repository Configuration
REACT_APP_DEFAULT_REPO=Trancendos/trancendos-ecosystem

# Feature Flags
REACT_APP_ENABLE_AUDIT_LOG=true
REACT_APP_ENABLE_EXPORT=true
REACT_APP_ENABLE_SYNC=true
REACT_APP_ENABLE_ANALYTICS=false

# Security Configuration
REACT_APP_SESSION_TIMEOUT=3600000
REACT_APP_MAX_RETRY_ATTEMPTS=3
REACT_APP_RATE_LIMIT_THRESHOLD=5000

# Performance Configuration
REACT_APP_CACHE_DURATION=300000
REACT_APP_DEBOUNCE_DELAY=500
REACT_APP_PAGINATION_SIZE=50

# Logging Configuration
REACT_APP_LOG_LEVEL=info
REACT_APP_LOG_STORAGE=true
